package Day1;

public class Variables_Practise 
{

	public static void main(String[] args) 
	{
	
		int a; //  Declaration
		a=100; // Assignment
		
		int b = 200, c= 300;	
		
		System.out.println(a+b+c);
		
		a=200;
		
		System.out.println(a);
		
		String name = "feroz", Dep = "Testing";
		System.out.println(name+Testing);

	}

}
