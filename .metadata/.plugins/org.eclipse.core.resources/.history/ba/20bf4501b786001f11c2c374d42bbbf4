package day2;

public class AssignmentOperators {

	public static void main(String[] args) 
	
	// Assignment = +=  -= *= /= %=
	
	{
		
	 int a = 10;
	 	 a+=5; // a=a+5
	 	 System.out.println(a);
	 	 
	 int b = 10;
	 	 b-=5; // b=b-5
	 	 System.out.println(b);
	 	 
	 int c = 20;
	     c*=1; // c=c*1
	     System.out.println(c);

	}

}
