package day2;

public class IncreamentOperators {

	public static void main(String[] args) 
	{
	
		int a = 10;
		/*	a = a+1;
		System.out.println(a);
		
		a++;
		System.out.println(a);*/
		
	 int res = a++;
	 System.out.println(res);
	 
	 int incres = ++a;
	 System.out.println(incres);
		
		
		
	}

}
