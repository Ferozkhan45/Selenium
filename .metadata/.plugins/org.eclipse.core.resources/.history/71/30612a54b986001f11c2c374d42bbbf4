package day2;

public class TernaryOperator {

	public static void main(String[] args) 
	{
	
		// Var =( exp ) ? Result : Result 2;
		
		//String a = "Yes", b="no";
		
		int x = (20>100) ? 100 : 200;
		System.out.println(x);

		
		int age = 30;
		
		String ravi = (age>=17) ? Elgible : NotElgible;
		System.out.println(ravi);
		
	}

}
